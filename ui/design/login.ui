    box-shadow: 0px 15px 35px rgba(0, 0, 0, 0.1);
}

/* Logo/Title Styling */
#logoLabel {
    color: #2c3e50;
    font-size: 28px;
    font-weight: bold;
    margin-bottom: 10px;
}

#subtitleLabel {
    color: #7f8c8d;
    font-size: 14px;
    font-weight: normal;
    margin-bottom: 30px;
}

/* Input Fields Styling */
QLineEdit {
    background-color: #f8f9fa;
    border: 2px solid #e9ecef;
    border-radius: 12px;
    padding: 15px 20px;
    font-size: 14px;
    color: #2c3e50;
    font-weight: 500;
}

QLineEdit:focus {
    border-color: #667eea;
    background-color: white;
    outline: none;
}

QLineEdit:hover {
    border-color: #adb5bd;
    background-color: white;
}

/* Input Field Icons */
#usernameLineEdit {
    padding-left: 50px;
    background-image: url(:/icons/user.png);
    background-repeat: no-repeat;
    background-position: left 15px center;
}

#passwordLineEdit {
    padding-left: 50px;
    background-image: url(:/icons/lock.png);
    background-repeat: no-repeat;
    background-position: left 15px center;
}

/* Checkbox Styling */
QCheckBox {
    color: #6c757d;
    font-size: 13px;
    spacing: 8px;
}

QCheckBox::indicator {
    width: 18px;
    height: 18px;
    border-radius: 4px;
    border: 2px solid #ced4da;
    background-color: white;
}

QCheckBox::indicator:checked {
    background-color: #667eea;
    border-color: #667eea;
    image: url(:/icons/check.png);
}

QCheckBox::indicator:hover {
    border-color: #667eea;
}

/* Login Button Styling */
#loginButton {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                               stop:0 #667eea, stop:1 #764ba2);
    color: white;
    border: none;
    border-radius: 12px;
    padding: 15px;
    font-size: 16px;
    font-weight: bold;
    text-transform: uppercase;
    letter-spacing: 1px;
}

#loginButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                               stop:0 #5a67d8, stop:1 #6b46c1);
    transform: translateY(-2px);
}

#loginButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                               stop:0 #4c51bf, stop:1 #553c9a);
}

/* Cancel Button Styling */
#cancelButton {
    background-color: transparent;
    color: #6c757d;
    border: 2px solid #e9ecef;
    border-radius: 12px;
    padding: 15px;
    font-size: 14px;
    font-weight: 600;
}

#cancelButton:hover {
    background-color: #f8f9fa;
    border-color: #ced4da;
    color: #495057;
}

#cancelButton:pressed {
    background-color: #e9ecef;
}

/* Links Styling */
QLabel#forgotPasswordLabel {
    color: #667eea;
    font-size: 13px;
    text-decoration: underline;
}

QLabel#forgotPasswordLabel:hover {
    color: #5a67d8;
}

QLabel#registerLabel {
    color: #6c757d;
    font-size: 13px;
}

QLabel#registerLinkLabel {
    color: #667eea;
    font-size: 13px;
    font-weight: bold;
    text-decoration: underline;
}

QLabel#registerLinkLabel:hover {
    color: #5a67d8;
}

/* Divider Line */
#dividerLine {
    background-color: #e9ecef;
    border: none;
    height: 1px;
    margin: 20px 0px;
}

/* Social Login Buttons */
#googleButton, #facebookButton {
    background-color: white;
    border: 2px solid #e9ecef;
    border-radius: 10px;
    padding: 12px;
    font-size: 13px;
    font-weight: 600;
    color: #495057;
}

#googleButton:hover, #facebookButton:hover {
    border-color: #ced4da;
    background-color: #f8f9fa;
}

#googleButton:pressed, #facebookButton:pressed {
    background-color: #e9ecef;
}

/* Animation Effects */
QWidget {
    selection-background-color: #667eea;
}

/* Error Message Styling */
#errorLabel {
    color: #e74c3c;
    font-size: 12px;
    font-weight: 600;
    background-color: rgba(231, 76, 60, 0.1);
    border: 1px solid rgba(231, 76, 60, 0.3);
