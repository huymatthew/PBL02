   <string notr="true">/* Add Contract Dialog Styling */
QDialog {
    background-color: #f8f9fa;
    color: #212529;
}

/* Group Box Styling */
QGroupBox {
    font-weight: bold;
    font-size: 14px;
    color: #495057;
    border: 2px solid #ced4da;
    border-radius: 10px;
    margin-top: 1.5ex;
    padding-top: 10px;
    background-color: white;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 15px;
    padding: 0 10px 0 10px;
    background-color: white;
}

/* Form Labels */
QLabel {
    color: #495057;
    font-weight: bold;
    font-size: 12px;
}

/* Input Fields Styling */
QDateEdit, QComboBox, QDoubleSpinBox, QLineEdit, QSpinBox, QTextEdit, QPlainTextEdit {
    background-color: white;
    border: 2px solid #ced4da;
    border-radius: 8px;
    padding: 10px 15px;
    font-size: 12px;
    min-height: 18px;
}

QDateEdit:focus, QComboBox:focus, QDoubleSpinBox:focus, QLineEdit:focus, QSpinBox:focus, QTextEdit:focus, QPlainTextEdit:focus {
    border-color: #007bff;
    background-color: #f0f8ff;
}

QDateEdit:hover, QComboBox:hover, QDoubleSpinBox:hover, QLineEdit:hover, QSpinBox:hover, QTextEdit:hover, QPlainTextEdit:hover {
    border-color: #6c757d;
}

/* ComboBox Arrow */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 25px;
    border-left: 1px solid #ced4da;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    background-color: #f8f9fa;
}

QComboBox::down-arrow {
    width: 15px;
    height: 15px;
    background-color: #6c757d;
}

QComboBox QAbstractItemView {
    background-color: white;
    border: 2px solid #ced4da;
    border-radius: 8px;
    selection-background-color: #007bff;
    selection-color: white;
}

/* Button Styling */
QPushButton {
    background-color: #007bff;
    color: white;
    border: none;
    border-radius: 8px;
    padding: 12px 25px;
    font-weight: bold;
    font-size: 12px;
    min-width: 100px;
}

QPushButton:hover {
    background-color: #0056b3;
}

QPushButton:pressed {
    background-color: #004085;
}

#saveButton {
    background-color: #28a745;
}

#saveButton:hover {
    background-color: #1e7e34;
}

#cancelButton {
    background-color: #6c757d;
}

#cancelButton:hover {
    background-color: #545b62;
}

#clearButton {
    background-color: #ffc107;
    color: #212529;
}

#clearButton:hover {
    background-color: #e0a800;
}

/* SpinBox Styling */
QSpinBox, QDoubleSpinBox {
    padding-right: 30px;
}

QSpinBox::up-button, QDoubleSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #ced4da;
    border-bottom: 1px solid #ced4da;
    border-top-right-radius: 8px;
    background-color: #f8f9fa;
}

QSpinBox::down-button, QDoubleSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    width: 20px;
    border-left: 1px solid #ced4da;
    border-bottom-right-radius: 8px;
    background-color: #f8f9fa;
}

QSpinBox::up-arrow, QDoubleSpinBox::up-arrow {
    width: 10px;
    height: 10px;
    background-color: #6c757d;
}

QSpinBox::down-arrow, QDoubleSpinBox::down-arrow {
    width: 10px;
    height: 10px;
    background-color: #6c757d;
}

