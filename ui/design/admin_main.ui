  <property name="styleSheet">
   <string notr="true">/* Admin Window Styling */
QMainWindow {
    background-color: #f8f9fa;
    color: #212529;
}

/* Menu Bar Styling */
QMenuBar {
    background-color: #343a40;
    color: white;
    border-bottom: 3px solid #495057;
    padding: 6px;
    font-size: 13px;
    font-weight: bold;
}

QMenuBar::item {
    background-color: transparent;
    padding: 10px 15px;
    border-radius: 5px;
    margin: 2px;
}

QMenuBar::item:selected {
    background-color: #007bff;
}

QMenuBar::item:pressed {
    background-color: #0056b3;
}

QMenu {
    background-color: white;
    border: 2px solid #dee2e6;
    border-radius: 8px;
    padding: 6px;
    font-size: 12px;
}

QMenu::item {
    padding: 10px 30px;
    margin: 2px;
    border-radius: 6px;
}

QMenu::item:selected {
    background-color: #007bff;
    color: white;
}

QMenu::separator {
    height: 2px;
    background-color: #dee2e6;
    margin: 6px 20px;
}

/* Tool Bar Styling */
QToolBar {
    background-color: #e9ecef;
    border: none;
    border-bottom: 2px solid #dee2e6;
    spacing: 10px;
    padding: 10px;
}

QToolButton {
    background-color: #ffffff;
    border: 2px solid #ced4da;
    border-radius: 10px;
    padding: 12px 16px;
    min-width: 100px;
    font-weight: bold;
    font-size: 11px;
}

QToolButton:hover {
    background-color: #007bff;
    color: white;
    border-color: #0056b3;
}

QToolButton:pressed {
    background-color: #0056b3;
}

/* Tab Widget Styling */
QTabWidget::pane {
    border: 2px solid #dee2e6;
    border-radius: 10px;
    background-color: white;
    padding: 8px;
}

QTabBar::tab {
    background-color: #f8f9fa;
    border: 2px solid #dee2e6;
    border-bottom: none;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    padding: 12px 20px;
    margin-right: 2px;
    font-weight: bold;
    font-size: 12px;
    color: #495057;
}

QTabBar::tab:selected {
    background-color: white;
    color: #007bff;
    border-color: #007bff;
    border-bottom: 2px solid white;
}

QTabBar::tab:hover {
    background-color: #e9ecef;
}

/* Table View Styling */
QTableView {
    background-color: white;
    alternate-background-color: #f8f9fa;
    gridline-color: #dee2e6;
    border: 2px solid #e9ecef;
    border-radius: 10px;
    selection-background-color: #007bff;
    selection-color: white;
    font-size: 11px;
}

QTableView::item {
    padding: 10px;
    border-bottom: 1px solid #e9ecef;
}

QTableView::item:selected {
    background-color: #007bff;
    color: white;
}

QHeaderView::section {
    background-color: #495057;
    color: white;
    padding: 12px;
    border: none;
    font-weight: bold;
    font-size: 12px;
}

QHeaderView::section:hover {
    background-color: #343a40;
}

/* Group Box Styling */
QGroupBox {
    font-weight: bold;
    font-size: 14px;
    color: #495057;
    border: 2px solid #ced4da;
    border-radius: 10px;
    margin-top: 1.5ex;
    padding-top: 10px;
    background-color: white;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 15px;
    padding: 0 10px 0 10px;
    background-color: white;
}

/* Form Labels */
QLabel {
    color: #495057;
    font-weight: bold;
    font-size: 12px;
}

/* Input Fields Styling */
QDateEdit, QComboBox, QDoubleSpinBox, QLineEdit, QSpinBox {
    background-color: white;
    border: 2px solid #ced4da;
    border-radius: 8px;
    padding: 10px 15px;
    font-size: 12px;
    min-height: 18px;
}

QDateEdit:focus, QComboBox:focus, QDoubleSpinBox:focus, QLineEdit:focus, QSpinBox:focus {
    border-color: #007bff;
    background-color: #f0f8ff;
}

QDateEdit:hover, QComboBox:hover, QDoubleSpinBox:hover, QLineEdit:hover, QSpinBox:hover {
    border-color: #6c757d;
}

/* ComboBox Arrow */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 25px;
    border-left: 1px solid #ced4da;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    background-color: #f8f9fa;
}

QComboBox::down-arrow {
    width: 15px;
    height: 15px;
    background-color: #6c757d;
}

QComboBox QAbstractItemView {
    background-color: white;
    border: 2px solid #ced4da;
    border-radius: 8px;
    selection-background-color: #007bff;
    selection-color: white;
}

/* Button Styling */
QPushButton {
    background-color: #007bff;
    color: white;
    border: none;
    border-radius: 8px;
    padding: 12px 25px;
    font-weight: bold;
    font-size: 12px;
    min-width: 100px;
}

QPushButton:hover {
    background-color: #0056b3;
}

QPushButton:pressed {
    background-color: #004085;
}

#addButton {
    background-color: #28a745;
}

#addButton:hover {
    background-color: #1e7e34;
}

#editButton {
    background-color: #ffc107;
    color: #212529;
}

#editButton:hover {
    background-color: #e0a800;
}

#deleteButton {
    background-color: #dc3545;
}

#deleteButton:hover {
    background-color: #c82333;
}

#searchButton {
    background-color: #17a2b8;
}

#searchButton:hover {
    background-color: #138496;
}

/* Status Bar Styling */
QStatusBar {
    background-color: #495057;
    color: white;
    border-top: 3px solid #343a40;
    padding: 6px;
}

QStatusBar QLabel {
    color: white;
    font-weight: bold;
    font-size: 13px;
    padding: 6px 12px;
}

/* Search Box */
#searchBox {
    background-color: white;
    border: 2px solid #ced4da;
    border-radius: 8px;
    padding: 8px 12px;
    min-width: 200px;
}
